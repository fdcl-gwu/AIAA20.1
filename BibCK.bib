booktitle = {Collection of Technical Papers - AIAA/ASME/ASCE/AHS/ASC Structures, Structural Dynamics and Materials Conference},
keywords = {Flexible wings;Flight dynamics;Rigid wings;Structu},
title = {{Flight dynamic stability of a flapping wing Micro Air Vehicle in hover}},
