journal = {Bioinspiration {\&} Biomimetics},
month = {jan},
number = {1},
pages = {016002},
pmid = {25561166},
title = {{The need for higher-order averaging in the stability analysis of hovering, flapping-wing flight}},
url = {http://stacks.iop.org/1748-3190/10/i=1/a=016002?key=crossref.a9dde5ec9f3ce7a36aae697ed18cda62},
volume = {10},
year = {2015}
