issn = {0924-090X},
journal = {Nonlinear Dynamics},
keywords = {,Design,Differential equations,Kinematics,Models,aerodynamic modeling,and,averaging,design,flapping mavs,floquet,high frequency periodic control,insects,kinematic,leading edge,stability analysis,theory,vortex},
