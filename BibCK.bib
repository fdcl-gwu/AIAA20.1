issn = {1748-3190},
journal = {Bioinspiration {\&} biomimetics},
keywords = {Animal,Animal: physiology,Animals,Biological,Biomimetics,Biomimetics: methods,Computer Simulation,Flight,Models,Nonlinear Dynamics,Species Specificity,Wing,Wing: physiology},
month = {mar},
number = {1},
