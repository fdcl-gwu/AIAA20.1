pages = {969--981},
title = {{Modeling and Simulation of Nonlinear Dynamics of Flapping Wing Micro Air Vehicles}},
url = {http://arc.aiaa.org/doi/abs/10.2514/1.J050649},
volume = {49},
year = {2011}
