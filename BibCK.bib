issn = {1091-6490},
pages = {1--6},
pmid = {25049376},
title = {{Predicting fruit fly's sensing rate with insect flight simulations.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25049376},
year = {2014}
}
