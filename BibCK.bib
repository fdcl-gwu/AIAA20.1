keywords = {Anatomic,Animal,Animal: physiology,Animals,Biological,Biomechanical Phenomena,Computer Simulation,Flight,Insects,Insects: physiology,Models,Movement,Wing,Wing: physiology},
month = {feb},
number = {Pt 3},
pages = {432--44},
pmid = {21228202},
title = {{Aerodynamic effects of corrugation in flapping insect wings in hovering flight.}},
