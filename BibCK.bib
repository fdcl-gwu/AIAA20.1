@article{Tanaka2006,
abstract = {In this paper, we show an approach to elucidate the free flight of an insect using a simulation. We modeled a fly, Drosophila, by using aerodynamics, body dynamics, and control theory. The modeled virtual insect performs free flight in virtual space generated by a computer. We simulated the free flight of a virtual insect having two dimensions and two degrees of freedom. The flight pass and flight velocity of the virtual insect during a free flight were calculated by Newton's equations of motion. The aerodynamic force generated by the flapping motion of the virtual insect was estimated by using the blade element theory. An optimal regulator theory was used as a control law. The flight pass from the initial position to the target position and the wing motion was obtained from the results of the free flight simulation of the virtual insect. We can presume the wing motion of an insect in free flight by using the flight simulation of a virtual insect. These results have suggested that the approach 
based on the simulation is effective in elucidating the free flight of an insect. Copyright {\&}copy; 2006 by The Japan Society of Mechanical Engineers.},
author = {Tanaka, Futoshi and Ohmi, Toshiatsu and Kuroda, Shigeaki and Hirasawa, Kazuhiro},
journal = {JSME International Journal, Series C: Mechanical Systems, Machine Elements and Manufacturing},
keywords = {Aerodynamics;Control theory;Degrees of freedom (me},
number = {2},
pages = {556--561},
title = {{Flight control study of an virtual insect by a simulation}},
volume = {49},
year = {2006}
}
