journal = {Bioinspiration {\&} biomimetics},
keywords = {Aircraft,Animals,Biomimetic Materials,Computer Simulation,Equipment Design,Feedback, Physiological,Feedback, Physiological: physiology,Flight, Animal,Flight, Animal: physiology,Insects,Insects: physiology,Models, Biological,Robotics,Robotics: instrumentation,Wing,Wing: physiology},
month = {dec},
