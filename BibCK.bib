keywords = {constrained optimisation,elongated-body theory,swimming kinematics},
month = {apr},
pages = {205--218},
title = {{Optimal Strouhal number for swimming animals}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0889974612000503 http://arxiv.org/abs/1102.0223},
